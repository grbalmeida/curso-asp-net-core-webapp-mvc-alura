<!DOCTYPE html>

<html lang="pt-br">
    <head>
        <meta charset="utf-8" />
        <title>Livros</title>
    </head>
    <body>
        <ul>
            @foreach (var livro in ViewBag.Livros)
            {
                <li>@livro.Titulo - @livro.Autor</li>
            }
        </ul>
    </body>
</html>